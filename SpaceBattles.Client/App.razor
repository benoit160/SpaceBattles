@using Microsoft.AspNetCore.Components.Routing

<Router AppAssembly="@typeof(UI.Pages.NotFound).Assembly" NotFoundPage="typeof(UI.Pages.NotFound)">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(UI.Layout.MainLayout)"/>
        <FocusOnNavigate RouteData="@routeData" Selector="h1"/>
    </Found>
</Router>